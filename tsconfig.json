{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "es5",
            "es2015",
            "es2016"
        ],
        "jsx": "react",
        "noImplicitReturns": false,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "sourceMap": true,
        "allowJs": false,
        "declaration": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}
